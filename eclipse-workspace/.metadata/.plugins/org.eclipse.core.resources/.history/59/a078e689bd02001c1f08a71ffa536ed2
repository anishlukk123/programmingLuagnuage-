package LinkedList;


public class hello {
	
	
public static void main(String[] args) {
		
	
	
	 var  list= new LinkedList();
	  list.addlast(3);
	  list.addlast(1);
	  list.addlast(0);
	 System.out.println(list.indexof(3));
	 System.out.println(list.indexof(1));
	 System.out.println(list.indexof(0));
	 System.out.println(list.size());
	 
	 
	}


}