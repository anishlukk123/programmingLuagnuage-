package logicprograming;

import java.util.Scanner;

public class SumRange {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		// test case 5 to 8 
		SumofRange();

	}

	private static void SumofRange() {
		
		Scanner scan = new Scanner(System.in);
		System.out.print("enter a number starting");
	int n =  scan.nextInt();
	
	System.out.print("enter a number ending");
	int m =  scan.nextInt();
	int sum1 = 0;
		
		for(int i=n;i<=m;i++)
		{
		 sum1 = sum1 +1;
				}
		
	}

}
