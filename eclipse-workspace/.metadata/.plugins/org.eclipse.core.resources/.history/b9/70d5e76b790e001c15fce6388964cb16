package Queues;

import java.util.Arrays;

public class ArrayQueue {
	private int items[];
	int front;
	int back = items.length;
	int count;
	
	public ArrayQueue(int size)
	{
		items = new int[size];

	}
	public void enque(int data)
	{
		
	 items[back--] = data;
	 count++;
	}
	
	
	public String toString()
	{
		return  Arrays.toString(items);
	}

}
