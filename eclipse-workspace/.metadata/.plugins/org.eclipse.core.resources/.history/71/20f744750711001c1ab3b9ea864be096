package Arrays;

public class Array {
	
	 int[]items;
	 int count;
	 
	 public Array(int length)

	 {
		 items = new int[length];
		 count =0;
	 }
	 public void insert(int data)
	 {
		 while(count!=items.length)
		 {
			 items[count] = data;
			 count++;
		 }
		  
	 }
	 
	 public  void Tostring()
	 {
		 for(int i =0;i<items.length;i++)
		 {
			 System.out.println(items[i]);
		 }
	 }
}
