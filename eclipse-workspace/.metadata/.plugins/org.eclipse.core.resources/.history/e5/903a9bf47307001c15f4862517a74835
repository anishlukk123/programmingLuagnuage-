package LinkedList;

import java.util.Arrays;

public class hello {
	
	
public static void main(String[] args) {
		
	
	
	 var  list= new LinkedList();
	 list.addfirst(3);
	 list.addlast(0);
	 list.addlast(1);
	 list.reverse();
	 
       System.out.println(Arrays.toString(list.toArray()));

	 
	 
	}


}