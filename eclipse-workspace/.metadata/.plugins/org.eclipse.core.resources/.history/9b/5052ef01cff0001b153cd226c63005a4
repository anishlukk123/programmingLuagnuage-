package arrays;

public class arrays {

	int num[];
	private int count;

	arrays(int length) {
		num = new int[length];
	}

	public void print() {
		for (int i = 0; i < count; i++) {
			System.out.println(num[i] + "");
		}
	}

	public void insert(int item) {
		if (num.length == count) {
			System.out.println("the array is full");
		} else {
			num[count] = item;
			count++;

		}
	}
}